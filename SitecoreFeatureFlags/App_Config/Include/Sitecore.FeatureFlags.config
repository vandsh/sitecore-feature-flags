<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <pipelines>
      <getPlaceholderRenderings>
        <processor patch:after="*[@type='Sitecore.Pipelines.GetPlaceholderRenderings.GetAllowedRenderings, Sitecore.Kernel']" type="SitecoreFeatureFlags.Pipelines.GetActiveModules, SitecoreFeatureFlags" />
      </getPlaceholderRenderings>
      <getLookupSourceItems>
        <processor patch:after="*[@type='Sitecore.Pipelines.GetLookupSourceItems.ProcessQuerySource, Sitecore.Kernel']" type="SitecoreFeatureFlags.Pipelines.GetAllowedOptions, SitecoreFeatureFlags" />
      </getLookupSourceItems>
    </pipelines>
  </sitecore>
</configuration>